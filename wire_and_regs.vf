wire .temp7 ?: .temp0 .temp3 WB_valid
reg M2_isAdrp = M2_isAdrp
reg WB_isAdrp = .temp6
reg WB_stall = WB_stall
reg M2_valid = M2_valid
reg WB_valid = .temp7
